@model IEnumerable<ETickets.Models.Movie>

@{
    ViewData["Title"] = "Search";
}

<h2>Search Results</h2>

@if (Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <!-- Other columns -->
            </tr>
        </thead>
        <tbody>
            @foreach (var movie in Model)
            {
                <tr>
                    <td>@movie.Name</td>
                    <!-- Other columns -->
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No movies found.</p>
}
